# Database Configuration
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@localhost:5432/fund_tracker
DB_USER=postgres
DB_PASSWORD=YOUR_PASSWORD
DB_HOST=localhost
DB_PORT=5432
DB_NAME=fund_tracker

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# E-Finance Documentation URL
API_REFERENCE_efinance_documentation_url=https://efinance.readthedocs.io/en/latest/api.html

# Tushare Pro API Token
# 获取方式：https://tushare.pro/register
# 注意：请勿将 token 提交到版本控制系统
TUSHARE_TOKEN=your_tushare_token_here

# Scheduler Configuration
SCHEDULER_ENABLED=true
SCHEDULER_HOUR=0
SCHEDULER_MINUTE=0

# Efinance API Configuration
EFINANCE_TIMEOUT=15
EFINANCE_MAX_RETRIES=3
EFINANCE_RETRY_BACKOFF=1.0

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_DECODE_RESPONSES=true
STOCK_NAME_CACHE_TTL=86400
